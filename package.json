{
  "name": "tools-ui",
  "version": "1.1.7",
  "description": "封装一些业务中常用的组件和工具",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "jest --watch",
    "init": "node build/bin/init",
    "build:theme": "gulp --gulpfile build/bin/theme.js",
    "build:components": "cross-env NODE_ENV=production webpack --config build/bin/build",
    "lib": "npm run init && npm run build:components && npm run build:theme",
    "docs:dev": "vuepress dev docs --open",
    "docs:build": "vuepress build docs",
    "lint": "eslint --ext .js --ext .vue packages/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue packages/",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "element-ui": "^2.15.3",
    "vue": "^2.6.11",
    "vue-router": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vuepress/plugin-back-to-top": "^2.0.0-beta.16",
    "@vuepress/plugin-register-components": "^1.8.2",
    "autodll-webpack-plugin": "^0.4.2",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chalkercli": "^1.6.4",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.31.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "7.14.0",
    "eslint-webpack-plugin": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "jest": "27.0.5",
    "@types/jest": "^26.0.24",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.14.1",
    "ora": "^5.4.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.3.0",
    "style-loader": "2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-loader": "15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^1.8.2",
    "vuepress-plugin-demo-container": "^0.2.0",
    "vuepress-plugin-gotop-plus": "^1.0.4",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "author": "qc-z",
  "license": "ISC",
  "keywords": [
    "vue",
    "components"
  ],
  "files": [
    "lib",
    "packages",
    "package.json"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qc-z/tools-ui"
  },
  "homepage": "https://qc-z.github.io/tools-ui"
}
